!!! DO NOT USE DATA SOURCE FOLDER FOR TRAINING DATA !!!

python tensorflow11/tensorflow/tensorflow/examples/image_retraining/retrain.py \
--bottleneck_dir=heroes2data/bottlenecks \
--how_many_training_steps 500 \
--model_dir=heroes2data/inception \
--output_graph=heroes2data/retrained_graph.pb \
--output_labels=heroes2data/retrained_labels.txt \
--image_dir heroes2


